<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" version="2.0">
<channel>
  <title><%= Disharmony::Config['show_info']['title'] %> </title>
  <link>http://www.rollins-archive.com</link>
  <language>en-us</language>
  <copyright>&#x2117; &amp; &#xA9; KCRW &amp; Henry Rollins</copyright>
  <itunes:author>Henry Rollins</itunes:author>
  <itunes:image href="<%= request.url %>himh.jpg" />
  <itunes:category text="Good Music" />
<% @shows.each do |show| %> 
<item>
  <title><%= show.title %></title>
  <enclosure url="<%= request.url + show.url %>" type="audio/mpeg" />
  <guid><%= request.url + show.url %></guid>
  <pubDate><%= show.air_date.rfc2822 %></pubDate>

  <description>
<![CDATA[
Track list:
<%= show.track_list %>
]]>
  </description>
</item>
<% end %>
</channel>
</rss>
